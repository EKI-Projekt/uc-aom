{
  "manifestVersion": "0.1",
  "version": "0.1.0-1",
  "title": "uc-addon-communication-receiver",
  "description": "AddOn that tests communication between AddOns",
  "logo": "logo.png",
  "services": {
    "ucaomtest-addon-communication-receiver": {
      "type": "docker-compose",
      "config": {
        "image": "test/uc-aom-communication-receiver:0.1",
        "containerName": "uc-addon-communication-receiver",
        "networkMode": "internal-bridge",
        "stdinOpen": true,
        "tty": true,
        "command": [
          "/bin/ash"
        ]
      }
    }
  },
  "platform": [
    "ucg",
    "ucm",
    "ucu"
  ],
  "vendor": {
    "name": "Weidmüller GmbH & Co KG",
    "url": "https://www.weidmueller.de",
    "email": "datenschutz@weidmueller.de",
    "street": "Klingenbergstraße 26",
    "zip": "32758",
    "city": "Detmold",
    "country": "Germany"
  }
}
